<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ql.animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TweenAnimationActivity" />
        <activity android:name=".activity.FirstActivity" />
        <activity android:name=".activity.SecondActivity" />
        <activity android:name=".activity.FragmentAnimationActivity" />
        <activity android:name=".activity.ViewGroupChildAnimationActivity" />
        <activity android:name=".activity.FrameAnimationActivity" />
        <activity android:name=".activity.ValueAnimatorActivity" />
        <activity android:name=".activity.ObjectAnimatorActivity" />
        <activity android:name=".activity.AnimatorSetActivity" />
        <activity android:name=".activity.ViewPropertyAnimatorActivity"/>
        <activity android:name=".activity.InterpolatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>